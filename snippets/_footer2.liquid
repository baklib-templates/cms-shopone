{% assign footer_menus = site.settings.footer_menu | newline_to_br | split: '<br />' %}

<footer class="bg-accent text-gray-400 py-12 px-4">
      <div class="mx-auto max-w-6xl flex justify-between">
          <div class="">
              <h3 class="text-sm font-semibold text-gray-100 mb-4">Join us for exclusive stories and special promotions.</h3>
              <div class="">
                <form method="post" action="https://ringconn.com/contact#footer-newsletter" id="footer-newsletter" accept-charset="UTF-8" class="flex w-[160px]" data-persist-bound="true">
                  <input type="hidden" name="form_type" value="customer">
                  <input type="hidden" name="utf8" value="âœ“">
                  <input type="hidden" name="contact[tags]" value="newsletter">
                  <input type="email" dir="ltr" name="contact[email]" autocomplete="email" enterkeyhint="send" placeholder="E-mail" class="flex-grow bg-gray-800 border border-gray-700 rounded-l-md py-2 px-4 text-white focus:outline-none focus:ring-2 focus:ring-blue-500">
                  <button class="bg-gray-700 hover:bg-gray-600 text-white py-2 px-4 rounded-r-md">
                      <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg>
                  </button>
                </form>
              </div>

              <div class="mt-8">
                  <h3 class="text-sm font-semibold text-white mb-4">Download App</h3>
                  <div class="flex space-x-4">
                      <a href="https://apps.apple.com/us/app/ringconn/id6444470583" class="inline-block">
                          <img src="https://ringconn.com/cdn/shop/files/app_store.png?v=1730898558&amp;width=3000" class="h-10" sizes="100vw">
                      </a>
                      <a href="https://play.google.com/store/apps/details?id=com.gdjztech.ringconn&hl=en_US&pli=1&ref=judge.me&sub_id=" class="inline-block">
                          <img src="https://ringconn.com/cdn/shop/files/goole_play.png?v=1730898557&amp;width=3000" class="h-10" sizes="100vw">
                      </a>
                  </div>
              </div>
          </div>

          <div class="grid grid-cols-2 md:grid-cols-3 gag-4 xl:gap-8">
            {% for menu_line in footer_menus %}
              {%  assign p_menu_arr = menu_line | split: '{' %}
              {%  assign parent_menu = p_menu_arr[0] | split: '[' %}
              {%  assign sub_menus = p_menu_arr[1] | split: '|' %}
              {%  assign sub_menus_count =  sub_menus | size %}

              {% if sub_menus_count > 0 %}
                <div class="flex flex-col" x-data="{ open: false }" @mouseenter="open = true" @mouseleave="open = false">
                    <a class="font-semibold" href="{{ parent_menu[1] | default: 'javascript:void(0);' }}" :aria-expanded="open">{{ parent_menu[0] }}</a>
                    <!-- 2nd level menu -->
                    <ul class="">
                      {% for second_menu in sub_menus %}
                        {%  assign second_menu_arr = second_menu | split: '[' %}
                        <li>
                            <a class="text-sm" href="{{ second_menu_arr[1] | default: '#'}}">
                                <span class="whitespace-nowrap">{{ second_menu_arr[0] }}</span>
                            </a>
                        </li>
                      {% endfor %}
                    </ul>
                </div>
              {% else %}
                <div class="">
                  <a class="" href="{{ parent_menu[1] | default: '#'}}">{{ parent_menu[0] }}</a>
                </div>
              {% endif %}
            {% endfor %}
          </div>
      </div>

